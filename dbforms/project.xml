<?xml version="1.0" encoding="UTF-8"?>

<project>
  <pomVersion>3</pomVersion>
  <id>dbforms</id>
  <name>DbForms</name>
  <currentVersion>1.1.2-dev</currentVersion>
  <organization>
    <name>DbForms</name>
    <url>http://www.dbforms.org</url>
    <logo>http://www.dbforms.org/img/logo.gif</logo>
  </organization>
  <inceptionYear>2002</inceptionYear>
  <package>org.dbforms</package>
  <logo></logo>

  <!-- Gump integration -->
  <gumpRepositoryId></gumpRepositoryId>

  <description>DbForms enables developers to build sophisticated web-based 
  database driven applications in very short time and with very little efforts. </description>
  <shortDescription>DbForms provides a form interface to databases</shortDescription>
  <url>http://www.dbforms.org</url>
  <issueTrackingUrl></issueTrackingUrl>
  <siteAddress></siteAddress>
  <siteDirectory></siteDirectory>
  <distributionDirectory></distributionDirectory>


  <repository>
    <connection></connection>
    <url></url>
  </repository>

  <mailingLists>
    <mailingList>
      <name>DbForms Interest</name>
      <subscribe>http://lists.sourceforge.net/lists/listinfo/jdbforms-interest</subscribe>
      <unsubscribe>http://lists.sourceforge.net/lists/listinfo/jdbforms-interest</unsubscribe>
      <archive></archive>
    </mailingList>
    <mailingList>
      <name>DbForms Developers</name>
      <subscribe>http://lists.sourceforge.net/lists/listinfo/jdbforms-developers</subscribe>
      <unsubscribe>http://lists.sourceforge.net/lists/listinfo/jdbforms-developers</unsubscribe>
      <archive></archive>
    </mailingList>    
  </mailingLists>

  <developers>
    <developer>
      <name>Eric Pugh</name>
      <id>epugh</id>
      <email>epugh@upstate.com</email>
      <organization/>
      <roles>
        <role>Java Developer</role>
      </roles>
    </developer>

  </developers>

  <dependencies>       
    <dependency>
      <id>cos</id>
      <jar>cos.jar</jar>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>        
    <dependency>
      <id>log4j</id>      
      <version>1.2.6</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>       
    <dependency>
      <id>j2ee</id>      
      <version/>
      <jar>j2ee.jar</jar>
    </dependency>
    <dependency>
      <id>commons-collections</id>      
      <version>2.0</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>        
    <dependency>
      <id>commons-dbcp</id>      
      <version>1.0-dev-20020806</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>        
    <dependency>
      <id>commons-pool</id>      
      <version>1.1-dev</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>        
    <dependency>
      <id>commons-digester</id>
      <version>1.3</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>        
    <dependency>
      <id>commons-logging</id>
      <version>1.0.1</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>        
    <dependency>
      <id>commons-validator</id>
      <version></version>
      <jar>commons-validator.jar</jar>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>          
    <dependency>
      <id>request</id>      
      <version/>
      <jar>request.jar</jar>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>
    <dependency>
      <id>xerces</id>      
      <version>1.4.4</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>                         
    <dependency>
      <id>commons-beanutils</id>      
      <version>1.4</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>        
    <dependency>
      <id>commons-util</id>
      <version>1.0-rc2-dev.20020814.103359</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>  
    
     <dependency>
      <id>xalan</id>
      <version>2.3.1</version>
    </dependency>       
   
    <!-- testing only -->
    <dependency>
      <id>hsqldb</id>
      <version>1.7.0</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>  
   <dependency>
      <id>junit</id>
      <version>3.7</version>
      <properties>        
        <war.bundle.jar>true</war.bundle.jar>
      </properties>
    </dependency>      
  </dependencies>
  

  <build>
    <nagEmailAddress></nagEmailAddress>
    <sourceDirectory>src</sourceDirectory>
    <unitTestSourceDirectory>tests/test</unitTestSourceDirectory>
    <aspectSourceDirectory></aspectSourceDirectory>

    <sourceModifications>
      <sourceModification>
        <excludes>
            <exclude>com/**/*.*</exclude>
        </excludes>
      </sourceModification>
    </sourceModifications>

    
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
        <include>**/Test*.java</include>
        
      </includes>
      <resources>
        <include>**/*.txt</include>
      </resources>
    </unitTest>
    
    <resources></resources>
    <jars></jars>
  </build>
</project>
